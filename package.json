    "@types/node": "^17.0.8",
    "@types/yargs": "^17.0.8",
    "jest": "^27.4.6",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.2",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  },
  "dependencies": {
    "yargs": "^17.3.1"
